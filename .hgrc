[alias]
latest = log --limit 10
[diff]
ignorews = true
ignoreblanklines = true
[defaults]
pull = -v
update = -v
fetch = -v
[extensions]
reviewboard = ~/SourceCode/mercurial-reviewboard
bphg.ext = 
color =
pager = 
fetch =
convert =
graphlog =
kerberos =
transplant=
rebase=
[reviewboard]
server = https://reviewboard.beatportcorp.net/
[pager]
pager = less -R
attend = glog,log,diff,help,heads,tags,branches
[ui]
username = Tavis Aitken <tavis.aitken@beatport.com>
email= tavis.aitken@beatport.com
merge=vimdiff
ignore=~/.hgignore
[web]
cacerts = /etc/hg-dummy-cert.pem
