[alias]
latest = log --limit 10
[diff]
ignorews = true
ignoreblanklines = true
[defaults]
pull = -v
update = -v
fetch = -v
[extensions]
reviewboard = ~/SourceCode/mercurial-reviewboard/reviewboard.py
bphg.ext = 
hgexternals = ~/bin/hgexternals.py
color =
pager = 
fetch =
convert =
hgext.graphlog =
hgext.kerberos =
children = 
progress =
rebase =
transplant =
[reviewboard]
server = https://reviewboard.beatportcorp.net/
[pager]
pager = less -R
attend = glog,log,diff,help,heads,tags,branches
[ui]
#username = Tavis Aitken <tavis.aitken@beatport.com>
username = Tavis Aitken
merge=vimdiff
ignore=~/.hgignore
