[alias]
latest = log --limit 10
[diff]
ignorews = true
ignoreblanklines = true
[defaults]
pull = -v
update = -v
fetch = -v
[extensions]
reviewboard = ~/SourceCode/mercurial-reviewboard/reviewboard.py
bphg.ext = 
hgexternals = ~/bin/hgexternals.py
color =
pager = 
fetch =
convert =
graphlog =
kerberos =
extdiff=
transplant=
rebase=
keychain=
[extdiff]
cmd.diffmerge = diffmerge
[reviewboard]
server = https://reviewboard.beatportcorp.net/
[pager]
pager = less -R
attend = glog,log,diff,help,heads,tags,branches
[ui]
username = Tavis Aitken <tavis.aitken@beatport.com>
merge = diffmerge
ignore=~/.hgignore
[merge-tools]
diffmerge.binary = False
